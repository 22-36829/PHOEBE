{
  "version": 2,
  "buildCommand": "CI=false npm ci && CI=false npm run build",
  "outputDirectory": "build",
  "installCommand": "npm ci",
  "framework": null,
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


